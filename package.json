{
  "name": "kumarajiva-desktop",
  "version": "1.0.0",
  "description": "Kumarajiva - Aliyun Cloud Player for Desktop",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none vite --host\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder",
    "electron:preview": "vite build && cross-env ELECTRON_IS_DEV=0 electron ."
  },
  "build": {
    "appId": "com.kumarajiva.desktop",
    "productName": "Kumarajiva",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "build"
    },
    "mac": {
      "category": "public.app-category.video",
      "identity": null
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "daisyui": "^4.10.2",
    "hls.js": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "wait-on": "^7.2.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
